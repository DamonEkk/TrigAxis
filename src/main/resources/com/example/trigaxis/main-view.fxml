<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.Group?>
<?import javafx.scene.layout.StackPane?>
<VBox alignment="CENTER" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.trigaxis.MainController" fx:id="mainVBox" VBox.vgrow="ALWAYS" minWidth="Infinity" minHeight="Infinity">
  <HBox fx:id="mainHBox" spacing="2" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS">
    <VBox fx:id="sideBar" maxHeight="Infinity" VBox.vgrow="ALWAYS" minWidth="300" alignment="CENTER" >
      <Label>Example side</Label>
    </VBox>
    <StackPane fx:id="mainCanvas" VBox.vgrow="ALWAYS" HBox.hgrow="ALWAYS">
      <Label fx:id="exampleText">Example Canvas</Label>

    </StackPane>
  </HBox>
</VBox>
